rd(this.state.date)\r\n    }\r\n\r\n\r\n    handleSubmit = (event) => {\r\n        event.preventDefault()\r\n        if(this.state.id === 0) {\r\n            this.props.addRecord(this.state)\r\n            this.clearState()\r\n        } else {\r\n            this.props.editRecord(this.state)\r\n            this.clearState()\r\n        }\r\n    }\r\n\r\n    render() {\r\n\r\n        return (\r\n            <div class=\"signin\">\r\n            <div class=\"input\">\r\n                <form onSubmit={this.handleSubmit} >\r\n                    <label>Date: </label>\r\n                    <input\r\n                        type=\"date\"\r\n                        name=\"date\" \r\n                        value={this.state.date}\r\n                        onChange={this.handleChange}/>\r\n                        <br />\r\n                    <label>Item Name: </label>\r\n                    <input \r\n                    type=\"text\"\r\n                        value={this.state.item_name} \r\n                        name=\"item_name\" \r\n                        onChange={this.handleChange}/>\r\n                        <br />\r\n                    <label>Item Calories: </label>\r\n                    <input \r\n                        type=\"number\"\r\n                        value={this.state.item_calories} \r\n                        name=\"item_calories\" \r\n                        onChange={this.handleChange}/>\r\n                        <div class=\"submit\">\r\n                            <input type=\"submit\" value=\"Add Item\" />\r\n                        </div>\r\n                </form>\r\n            </div>\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nexport default DailyRecordInput",["165","166"],"/mnt/c/Users/dstem/dev/Projects/Counting-Cookies/counting_frontend/src/reducers/usersReducer.js",[],"/mnt/c/Users/dstem/dev/Projects/Counting-Cookies/counting_frontend/src/actions/addVote.js",[],"/mnt/c/Users/dstem/dev/Projects/Counting-Cookies/coun